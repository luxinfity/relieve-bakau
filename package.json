{
    "name": "bakau",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "serve": "nodemon ./server.js",
        "server": "node ./server.js",
        "clean-install": "rm -rf node_modules package-lock.json && npm install",
        "migrate": "./scripts/migrations/migrate.sh",
        "migrate:rollback": "./scripts/migrations/rollback.sh",
        "migrate:refresh": "./scripts/migrations/refresh.sh",
        "db:seed": "./scripts/migrations/seed.sh",
        "tests": "NODE_ENV=test nyc --reporter=html --reporter=text ava --verbose",
        "tests-report": "open coverage/index.html",
        "lint": "eslint --fix src/**"
    },
    "dependencies": {
        "@hapi/joi": "^16.1.6",
        "@hapi/joi-date": "^2.0.0",
        "axios": "^0.21.1",
        "bcryptjs": "^2.4.3",
        "bluebird": "^3.5.3",
        "body-parser": "~1.18.2",
        "cors": "^2.8.4",
        "debug": "~2.6.9",
        "dotenv": "^5.0.1",
        "express": "^4.16.4",
        "express-rate-limit": "^2.11.0",
        "google-auth-library": "^2.0.1",
        "helmet": "^3.15.1",
        "ioredis": "^4.6.2",
        "jsonwebtoken": "^8.5.0",
        "lodash": "^4.17.21",
        "moment": "^2.29.2",
        "mongodb": "^3.1.13",
        "mongoose": "^5.7.5",
        "mongoose-uuid2": "^2.3.0",
        "morgan": "~1.9.0",
        "mysql2": "^1.6.5",
        "randomstring": "^1.1.5",
        "relieve-common": "git+https://github.com/RelieveID/relieve-common.git",
        "sequelize": "^5.3.0",
        "uuid": "^3.2.1"
    },
    "devDependencies": {
        "ava": "^1.3.1",
        "eslint": "^4.18.1",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-config-airbnb-base": "^13.0.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-react": "^7.12.4",
        "nodemon": "^1.18.10",
        "nyc": "^14.1.1",
        "sinon": "^7.2.4",
        "sinon-express-mock": "^2.1.0",
        "supertest": "^3.4.2"
    },
    "nyc": {
        "per-file": true,
        "lines": 95,
        "statements": 95,
        "functions": 95,
        "branches": 95,
        "reporter": [
            "lcov"
        ],
        "exclude": [
            "src/repositories/*.js",
            "tests/**"
        ]
    },
    "ava": {
        "files": [
            "tests/**/*.test.js"
        ],
        "failFast": false,
        "failWithoutAssertions": false,
        "tap": false,
        "powerAssert": true
    }
}
